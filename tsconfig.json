{
  "baseUrl": ".",
  "paths": {
    "resources/*": [
      "./*"
    ]
  },
  "compilerOptions": {
    "jsx": "react",
    "outDir": "build",
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "typeRoots": ["node_modules/@types"]
  },
  "include": [
    "./**/**.tsx",
    "./**/**.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
